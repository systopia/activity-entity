<?xml version="1.0"?>
<extension key="activity-entity" type="module">
  <file>activity_entity</file>
  <name>Activity Entity Connector</name>
  <description>Connect activities with other entities</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA GmbH</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/activity-entity</url>
    <url desc="Documentation">https://github.com/systopia/activity-entity</url>
    <url desc="Support">https://github.com/systopia/activity-entity/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-10-20</releaseDate>
  <version>0.1-dev</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.54</ver>
  </compatibility>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/ActivityEntity</namespace>
    <format>22.10.0</format>
    <angularModule>crmActivityEntity</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
</extension>
